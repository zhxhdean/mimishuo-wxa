page{
  background: #eee;
}
.body{
  background: #eee;
  font-size:24rpx;
  padding:10rpx 30rpx;
  box-sizing: border-box;
}
.action{
  background: #fff;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  font-size:32rpx;
  align-items: center;
  padding:0 60rpx;
}
.action text{
  color:#9D9D9D;
}
.action .submit{
  color:#29B500;
}
.virtualinfo{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  height: 100rpx;
  box-sizing: border-box;
}
.virtualinfo image{
  width: 100rpx;
  height: 100rpx;
}
.virtualinfo text{
  color:#9D9D9D;
  margin-left: 30rpx;
}
.virtualinfo span {
  color:#0056D7;
  margin-left: 50rpx;
  margin-top: 34rpx;
}
.virtualinfo .flag{
  width:32rpx;
  height: 22rpx;
  margin-left:10rpx;
  margin-top: 34rpx;
}
.text{
  font-size: 14px;
}
.userinfo {
  padding:10rpx;
}
.userinfo text{
  display: block;
}
.content{
  margin-top: 10rpx;
  background:#eee;
  width:100%;
  height: 120rpx;
}
.imglist{
  display: flex;
  flex-wrap: wrap;
}
.add{
  text-align: center;
  width: 200rpx;
  height: 200rpx;
  line-height: 160rpx;
  box-sizing: border-box;
  font-size:80px;
  color:#fff;
  background:#C9C9C9;
  border-radius: 0;
  font-weight: bolder;
  margin-left:0;
  margin-top:10rpx;
}
.imglist view{
  position: relative;
  margin-top:10rpx;
}
.imglist image {
  width: 200rpx;
  height: 200rpx;
  margin-right: 30rpx;
}
.imglist image i{
  position: absolute;
  display: block;
  left: 190rpx;
  top: 0rpx;
  color:#f20;
  width: 20rpx;
  height: 20rpx;
  font-weight: bold;
  transform: rotate(45deg) scale(1.5);
}